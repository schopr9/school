<p id="notice"><%= notice %></p>

<h1>Listing Adminforms</h1>

<table>
  <thead>
    <tr>
      <th>Name of student</th>
      <th>Gender</th>
      <th>Class</th>
      <th>Date of birth</th>
      <th>Category</th>
      <th>Nationality</th>
      <th>Father name</th>
      <th>Mother name</th>
      <th>Address line 1- string</th>
      <th>Address line2</th>
      <th>City</th>
      <th>State</th>
      <th>Pin code</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Father occupation -string</th>
      <th>Highest degree 1</th>
      <th>Annual income 1</th>
      <th>Mother occupation</th>
      <th>Highest degree 2</th>
      <th>Annual income 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adminforms.each do |adminform| %>
      <tr>
        <td><%= adminform.Name_of_student %></td>
        <td><%= adminform.Gender %></td>
        <td><%= adminform.Class %></td>
        <td><%= adminform.Date_of_birth %></td>
        <td><%= adminform.Category %></td>
        <td><%= adminform.Nationality %></td>
        <td><%= adminform.Father_name %></td>
        <td><%= adminform.Mother_name %></td>
        <td><%= adminform.Address_line_1-_string %></td>
        <td><%= adminform.Address_line2 %></td>
        <td><%= adminform.City %></td>
        <td><%= adminform.State %></td>
        <td><%= adminform.Pin_code %></td>
        <td><%= adminform.Email %></td>
        <td><%= adminform.Phone %></td>
        <td><%= adminform.Father_occupation_-string %></td>
        <td><%= adminform.Highest_degree_1 %></td>
        <td><%= adminform.Annual_income_1 %></td>
        <td><%= adminform.Mother_occupation %></td>
        <td><%= adminform.Highest_degree_2 %></td>
        <td><%= adminform.Annual_income_2 %></td>
        <td><%= link_to 'Show', adminform %></td>
        <td><%= link_to 'Edit', edit_adminform_path(adminform) %></td>
        <td><%= link_to 'Destroy', adminform, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

